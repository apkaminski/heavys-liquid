    "testimonial_grid_JFjyAb": {
      "type": "testimonial-grid",
      "blocks": {
        "column_WteqXH": {
          "type": "column",
          "settings": {
            "title": "",
            "label": "",
            "text": "<p><em><strong>\" <\/strong><\/em><em>These New Wireless Headphones Are Designed with Heavy Metal Fans In Mind<\/em><em><strong> \" <\/strong><\/em><em>- FORBES<\/em><\/p>"
          }
        },
        "column_zXcWgA": {
          "type": "column",
          "settings": {
            "title": "",
            "label": "",
            "text": "<p><em><strong>“ <\/strong><\/em><em>Fantastic sound, built with care for the users comfort. There's nothing better than being able to truly listen to how the songs were intended to be heard. Heavy's made it possible and it actually changes the songs I listen to <\/em><em><strong>”<\/strong><\/em><em> - Christian Rodrigue<\/em><\/p>"
          }
        },
        "column_QaQiEY": {
          "type": "column",
          "settings": {
            "title": "",
            "label": "",
            "text": "<p><em>“Epic as written about! After hours of use it is still very comfortable - feels like they are not there. You can hear all notes of songs high and super deep” <\/em><\/p><p><em>- Turner McLeod<\/em><\/p>"
          }
        },
        "column_677tYA": {
          "type": "column",
          "settings": {
            "title": "",
            "label": "",
            "text": "<p><em><strong>“ <\/strong><\/em><em>Best pair of headphones I’ve ever owned <\/em><em><strong>”<\/strong><\/em><em> - Mayon A<\/em><\/p>"
          }
        },
